#!/bin/bash
echo "__name__: ${BASH_SOURCE[0]}"
if [ -d "/boot/firmware" ] ;  then 
  sed -i -e '/dtoverlay=uno220-gpio/d' -e '/\[all\]/adtoverlay=uno220-gpio' /boot/firmware/config.txt
else
  sed -i -e '/dtoverlay=uno220-gpio/d' -e '/\[all\]/adtoverlay=uno220-gpio' /boot/config.txt
fi
sed -e '1,/^exit$/d' "$0" | tar -C / -xpzf - 
exit
